{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/users/delete/:userId",
      "destination": "/api/delete-user"
    },
    {
      "source": "/api/conductors/create",
      "destination": "/api/create-conductor"
    },
    {
      "source": "/((?!assets|vite\\.svg|api).*)",
      "destination": "/index.html"
    }
  ]
}
